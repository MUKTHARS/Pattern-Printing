#include <stdio.h>


int main()
{
   int i,j,k;
   printf("Enter The Number :\t");
   scanf("%d",&k);
   i=1;
   while(i<=k)
   {
       j=0;
       while(j<i)
       {
           printf("* ");
           j++;
       }
       while(j<=k-1)
       {
           printf("# ");
           j++;
       } 
       printf("\n");
       
    i++;
    }


   }
